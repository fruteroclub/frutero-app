# Para Wallet SDK
NEXT_PUBLIC_PARA_API_KEY=your_para_api_key_here
NEXT_PUBLIC_PARA_ENVIRONMENT=development

# Are.na API Configuration
# Obtén tu token personal en: https://dev.are.na/oauth/applications
ARENA_PERSONAL_ACCESS_TOKEN=your_arena_personal_access_token_here

# Are.na Channel Configuration
# Canal público para que la comunidad pueda agregar contenido
NEXT_PUBLIC_ARENA_PUBLIC_CHANNEL=your_public_channel_slug

# Canal privado/admin para contenido curado (requiere permisos)
NEXT_PUBLIC_ARENA_ADMIN_CHANNEL=your_admin_channel_slug